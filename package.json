{
  "name": "html-chunk-loader",
  "version": "0.3.12",
  "description": "Node Template Engine using HTML Comments",
  "main": "dist/index.js",
  "bin": "dist/cli/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "npm run build && jest",
    "prepare": "husky install",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "docs": "typedoc",
    "testCli": "yarn build && node dist/util/cli"
  },
  "files": [
    "lib/**/*",
    "dist/*",
    "dist/**/*"
  ],
  "keywords": [
    "html",
    "ssr",
    "template",
    "template engine",
    "engine",
    "parser",
    "ssg",
    "rendering"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/abschill/html-chunk-loader"
  },
  "author": "abschill",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "pinst": "^2.1.6",
    "ts-jest": "^27.0.5",
    "typedoc": "^0.22.6",
    "typedoc-plugin-markdown": "^3.11.3",
    "typescript": "^4.4.2"
  },
  "static_config": {
    "pathRoot": "views3",
    "partials": "layout",
    "templates": "pages",
    "outPath": "public",
    "loaderFile": "static-paths.js",
    "cleanup": true
  }
}
