{
  "name": "html-chunk-loader",
  "version": "0.0.91",
  "description": "Node Template Engine using HTML Comments",
  "main": "dist/index.js",
  "bin": "cli/entry.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "npm run build && jest",
    "prepare": "husky install",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "files": [
    "dist/**/*",
    "cli/**/*",
    "index.d.ts"
  ],
  "keywords": [
    "template",
    "engine",
    "parser",
    "ssr",
    "ssg",
    "rendering"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/abschill/html-chunk-loader"
  },
  "author": "abschill",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "pinst": "^2.1.6",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2"
  },
  "_static_config": {
    "root": "views",
    "partials": "partials",
    "templates": "pages",
    "outPath": "public",
    "loaderFile": "loader.js",
    "cleanup": true
  },
  "_partial_data": {
    "head": {
      "title": "Hello World",
      "desc": "Cool Description"
    },
    "footer": {
      "title": "Hello From Footer"
    }
  }
}
